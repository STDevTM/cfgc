{
    "manifestVersion": 1,
    "id": "stdev-cfgc-extension",
    "name": "Changelog Generation Tools",
    "version": "1.0.0",
    "publisher": "STDevTeam",
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Tools for generating changelog from git commits.",
    "categories": [
        "Build and release"
    ],
    "tags": [ "release", "tag", "git", "notes", "changelog", "git log", "commit" ],
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/stdevteam/cfgc/issues"
        },
        "repository": {
            "uri": "https://github.com/stdevteam/cfgc"
        },
        "issues": {
            "uri": "https://github.com/stdevteam/cfgc/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/stdevteam/cfgc"
    },
    "icons": {
        "default": "images/icon.png"        
    },
    "files": [
        {
            "path": "cfgc"
        }
    ],
    "contributions": [
        {
            "id": "cfgc",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "cfgc"
            }
        }
    ]
}
